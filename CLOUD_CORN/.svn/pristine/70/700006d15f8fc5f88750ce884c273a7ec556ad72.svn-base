@charset "utf-8";
li {
   list-style:none;
}

/* 찾아오시는 길 */
.locartion-wrap {}
.locartion-wrap .location-nav {margin:0 0 30px;}
.locartion-wrap .location-nav .tabs {margin:0 -5px; padding: 0;}
.locartion-wrap .location-nav .tabs:after {content:"";display:block;clear:both;}
.locartion-wrap .location-nav .tabs li {float:left;width:33.33333333%;padding:0 5px;}
.locartion-wrap .location-nav .tabs li a {display:block;text-decoration:none;text-align:center;font-size:18px;font-weight:400;line-height:30px;padding:10px;color:#fff;background:#686868;}
.locartion-wrap .location-nav .tabs li.active a {background:#ea5b63;}
.locartion-wrap .location-container {}
.locartion-wrap .location-container .map-zone {margin:0 0 30px;}
.locartion-wrap .location-container .map-zone iframe {display:block;width:100% !important;}
.locartion-wrap .location-container .addr-info {}
.locartion-wrap .location-container .addr-info table {width:100%;border-collapse:collapse;border-top:2px solid #383838;}
.locartion-wrap .location-container .addr-info table tbody tr th {font-size:16px;font-weight:400;line-height:22px;padding:19px 10px;color:#454545;border-bottom:1px solid #dedede;}
.locartion-wrap .location-container .addr-info table tbody tr td {font-size:16px;font-weight:300;line-height:22px;padding:10px 20px;color:#686868;border-bottom:1px solid #dedede;}
