    <!-- ======= Board list Section ======= -->
    <section class="contact" data-aos="fade-up" data-aos-easing="ease-in-out" data-aos-duration="500">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <form id="form_write" name="form_write" role="form" class="php-email-form" enctype="multipart/form-data">
              <div class="form-group">
                <textarea class="form-control" id="faq_question" name="faq_question" rows="5" placeholder="질문" PSS_BLANK></textarea>
              </div>
              <div class="form-group">
                <textarea class="form-control" name="faq_answer" id="faq_answer" rows="10" cols="100" placeholder="답변" style="width:100%; height:412px; display:none;" PSS_BLANK></textarea>
              </div>
              <div class="custom-file">
                <input type="file" class="custom-file-input" id="validatedCustomFile">
                <label class="custom-file-label" for="validatedCustomFile">파일선택1</label>
                <div class="invalid-feedback">파일 선택해 주세요</div>
              </div>
              <div class="text-center mt-3">
              <button class="btn btn-primary" type="submit">저장</button>
            </div>
              </form>
          </div>

        </div>

