package com.enc.dto;

import java.time.LocalDateTime;

public class User {
	
	String userid;  // 사용자 ID
	String password;  // 비밀번호
	String usernm;  // 담당자 이름
	LocalDateTime regdt;  // 등록날짜
	String statuscd;  // 상태코드
	LocalDateTime dropdt;  // 탈퇴날짜
	LocalDateTime modifydt;  // 수정날짜
	String hphone;  // 휴대전화번호
	String email;  // 이메일
	String ceonm;  // 사업자CEO이름
	String comp_name;  // 사업자 상호
	String biznum;  // 사업자등록번호
	String cor_num;  // 법인등록번호
	String comp_addr;  // 사업장주소
	String comp_tel;  // 사업장전화번호
	LocalDateTime addrovaldt;  //  승인날짜
	String comp_fax;  // 사업장FAX번호
	String ktcloudid;  // KT클라우드아이디
	
	public User() {
		// TODO Auto-generated constructor stub
	}

	public String getUserid() {
		return userid;
	}

	public void setUserid(String userid) {
		this.userid = userid;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getUsernm() {
		return usernm;
	}

	public void setUsernm(String usernm) {
		this.usernm = usernm;
	}

	public LocalDateTime getRegdt() {
		return regdt;
	}

	public void setRegdt(LocalDateTime regdt) {
		this.regdt = regdt;
	}

	public String getStatuscd() {
		return statuscd;
	}

	public void setStatuscd(String statuscd) {
		this.statuscd = statuscd;
	}

	public LocalDateTime getDropdt() {
		return dropdt;
	}

	public void setDropdt(LocalDateTime dropdt) {
		this.dropdt = dropdt;
	}

	public LocalDateTime getModifydt() {
		return modifydt;
	}

	public void setModifydt(LocalDateTime modifydt) {
		this.modifydt = modifydt;
	}

	public String getHphone() {
		return hphone;
	}

	public void setHphone(String hphone) {
		this.hphone = hphone;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getCeonm() {
		return ceonm;
	}

	public void setCeonm(String ceonm) {
		this.ceonm = ceonm;
	}

	public String getComp_name() {
		return comp_name;
	}

	public void setComp_name(String comp_name) {
		this.comp_name = comp_name;
	}

	public String getBiznum() {
		return biznum;
	}

	public void setBiznum(String biznum) {
		this.biznum = biznum;
	}

	public String getCor_num() {
		return cor_num;
	}

	public void setCor_num(String cor_num) {
		this.cor_num = cor_num;
	}

	public String getComp_addr() {
		return comp_addr;
	}

	public void setComp_addr(String comp_addr) {
		this.comp_addr = comp_addr;
	}
	
	public String getComp_tel() {
		return comp_tel;
	}

	public void setComp_tel(String comp_tel) {
		this.comp_tel = comp_tel;
	}

	public LocalDateTime getAddrovaldt() {
		return addrovaldt;
	}

	public void setAddrovaldt(LocalDateTime addrovaldt) {
		this.addrovaldt = addrovaldt;
	}

	public String getComp_fax() {
		return comp_fax;
	}

	public void setComp_fax(String comp_fax) {
		this.comp_fax = comp_fax;
	}

	public String getKtcloudid() {
		return ktcloudid;
	}

	public void setKtcloudid(String ktcloudid) {
		this.ktcloudid = ktcloudid;
	}

	
	
	
	
}
